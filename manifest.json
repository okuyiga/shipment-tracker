{
  "name": "Shipment Tracker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Extension for keeping track of shipments",
  "homepage_url": "http://djfdat.github.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Shipment Tracker",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "*://tools.usps.com/*"
	]
}